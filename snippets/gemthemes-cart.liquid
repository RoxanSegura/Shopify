                <span class="text_total">
                  Subtotal:
                </span>
                <span class="number_total">
                  {{ cart.total_price | money }}
                </span>
              </div>

              {% assign totalComparePrice = 0 %}
              {% for item in cart.items %}
                {% if item.variant.compare_at_price > item.variant.price and item.variant.compare_at_price > 0 %}
                  {% assign saveCompareItem = item.variant.compare_at_price | minus:item.variant.price %}
                  {% assign totalComparePriceItem = saveCompareItem | times:item.quantity %}
                  {% assign totalComparePrice = totalComparePrice | plus: totalComparePriceItem %}
                {% endif %}
              {% endfor %}
              
              {% assign saveMoney = totalComparePrice | plus:cart.total_discount %}
