
  
<section class="gt_section-4298086 " data-name="Product Details" keyword="product">
  
  {%- assign current_variant = product.selected_or_first_available_variant -%}
  <script type="application/json" class="ProductJson" data-id="{{product.id}}" data-variant="{{product.selected_or_first_available_variant.id}}">
      {{ product | json }}
  </script>
  {% unless current_variant == empty %}
    <script type="application/json"> 
        {{ current_variant | json }}
    </script>
  {% endunless %}
  
  {% assign currentPrice = current_variant.price | plus: 0 %}
  {% assign comparePrice = current_variant.compare_at_price | plus: 0 %}
  {% assign featured_image = current_variant.featured_image | default: product.featured_image %}

  <div class="position_hook">
    
  </div>
  <div class="gt_product">
    <div class="gt_container">
      <div class="gt_flex gt_flex-sm-column">
        <div class="gt_product-image">
          <div class="position_hook">
            
          </div>
          <div class="gt_product-img">
              <img class="gt_product-image--feature gt_lazyload" src="{{ featured_image.src | img_url: '100x' }}" data-id="{{featured_image.id}}" alt="{{ featured_image.alt | escape }}">
          </div>
          {% if product.images.size > 0%}
            <div class="owl-carousel gt_product-carousel">
              {%- for image in product.images -%}
                <div class="item gt_product-carousel--item">
                    {% assign classThumbnailImage = "" %}
                    {% if featured_image.id == image.id %}
                      {% assign classThumbnailImage = "gt_active" %}
                    {% endif %}
                    
                    <img class="{{classThumbnailImage}} gt_lazyload" src="{{ image.src | img_url: '100x' }}" data-id="{{image.id}}" alt="{{ image.alt | escape }}" data-original="{{ image.src | img_url: 'master' }}">            
                </div>
              {%- endfor -%}
            </div>
          {% endif %}
          <div class="position_hook">
            
          </div>
        </div>
        <div class="gt_product-content">
          <div class="gt_product-title">
            <div class="position_hook">
              
            </div>
            <a>
              <h1 class="gt_product-title--h2" itemprop="name">
                {{ product.title }}
              </h1>
            </a>

            
            
            
            
              
            
            
              <div class="position_hook">
                
              </div>
              
          </div>

          <div class="gt_product-body">
            <div class="position_hook">
              
            <div class="gt_flex gt_product-price">
              <div>
                <span class="gt_product-price--number" itemprop="price" data-currentprice="{{ product.selected_or_first_available_variant.price }}">
                  {{ product.selected_or_first_available_variant.price | money }}
                </span>  
                
                  {% if product.selected_or_first_available_variant.compare_at_price > 0 and product.selected_or_first_available_variant.compare_at_price > product.selected_or_first_available_variant.price %}                   
                  <span class="gt_product-price--compare" data-currentprice="{{ product.selected_or_first_available_variant.compare_at_price }}">
                    {{ product.selected_or_first_available_variant.compare_at_price | money }}
                  </span>
                  {% endif %}
                
              </div>

              {% if comparePrice > 0 and comparePrice > currentPrice %}
                {% assign classProductSaved = 'gt_active' %}
              {% else %}
                {% assign classProductSaved = '' %}
              {% endif %}
              
                <div class="gt_product-saved {{classProductSaved}}">
                  <span>
                    Saved
                  </span>
                  <span class="gt_product-saved--number">
                    {%- if comparePrice > 0 and comparePrice > currentPrice -%}
                      {%- assign saved = comparePrice | minus: currentPrice -%}
                      {%- assign comparePriceFloat = comparePrice | times: 1.0 -%}
                      {%- assign diff = saved | divided_by: comparePriceFloat -%}
                      {%- assign diff = diff | times: 100.0 -%}
                      {%- assign diff = diff | round -%}
                      {{ diff }}%
                    {%- endif -%}
                  </span>
                  <span class="gt_product-saved--price">
                    {%- if comparePrice > 0 and comparePrice > currentPrice -%}
                      {%- if saved > 0 -%}
                        ({{ saved | money }})
                      {%- endif -%}
                    {%- endif -%}
                  </span>
                </div>
              
            </div>
            <div class="position_hook">
              
            </div>
          
            {% form 'product', product, class:'product-form' %}
              <input type="hidden" name="id" class="gt_swatches--input" value="{{current_variant.id}}">
              <div class="gt_product-swatches">
                {% for option in product.options_with_values %}
                  {% if option.name != "Title" or option.values.size != 1 or option.values[0] != "Default Title"  %}
                    <div class="gt_product-swatches--item {% if option.values.size == 1%} variant_none {% endif %}" data-name="{{option.name}}">
                      <span class="gt_product-swatches--name">{{option.name}}</span>
                      <div class="gt_flex gt_flex-wrap gt_product-swatches--options">
                        {% for value in option.values %}
                          {% if value == option.selected_value %}
                            {% assign selectedClass = 'gt_swatches--select gt_active' %}
                          {% else %}
                            {% assign selectedClass = 'gt_swatches--select' %}
                          {% endif %}
                            <div class="{{selectedClass}}" data-name="{{option.name | replace: '"', "'"}}" data-value="{{value | replace: '"', "'"}}"><span>{{ value }}</span></div>
                        {% endfor %}
                      </div>
                    </div>
                  {% endif %}
                {% endfor %}

                {% if option.values.size > 1 %}
                  <div class="position_hook">
                    
                  </div>
                {% endif %}
              </div>

              {% if current_variant.available %}
              {% assign classQuantity = '' %}
              {% else %}
                {% assign classQuantity = 'gt_soldout' %}
              {% endif %}
              <div class="gt_product-quantity {{classQuantity}}">
                <span>
                  Quantity
                </span>
                <div class="gt_inline-flex gt_product-quantity--wrapper">
                  <div class="gt_flex gt_justify-c-center gt_align-i-center gt_product-quantity--minus">
                    <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                      <path fill-rule="evenodd" clip-rule="evenodd" d="M2 8C2 7.44772 2.44772 7 3 7L13 7C13.5523 7 14 7.44772 14 8C14 8.55228 13.5523 9 13 9L3 9C2.44772 9 2 8.55228 2 8Z" fill="#6D6D6D"/>
                    </svg>                          
                  </div>
                  <input type="number" class="input gt_product-quantity--number" value="1" min="1" name="quantity">
                  <div class="gt_flex gt_justify-c-center gt_align-i-center gt_product-quantity--plus">
                    <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                      <path fill-rule="evenodd" clip-rule="evenodd" d="M8 2C8.55228 2 9 2.44772 9 3V13C9 13.5523 8.55228 14 8 14C7.44772 14 7 13.5523 7 13V3C7 2.44772 7.44772 2 8 2Z" fill="#6D6D6D"/>
                      <path fill-rule="evenodd" clip-rule="evenodd" d="M2 8C2 7.44772 2.44772 7 3 7L13 7C13.5523 7 14 7.44772 14 8C14 8.55228 13.5523 9 13 9L3 9C2.44772 9 2 8.55228 2 8Z" fill="#6D6D6D"/>
                    </svg>                          
                  </div>
                </div>
              </div>

              {% if current_variant.available %}
                {% assign classButton = '' %}
              {% else %}
                {% assign classButton = 'gt_soldout' %}
              {% endif %}
              <div class="ro_widget"></div>
              
              
                <div class="gt_product-addtocart {{classButton}}" data-addtocart="BUY IT NOW!" data-soldout="SOLD OUT">
                <div class="position_hook">
                  
                </div>

                <button type="submit" name="add" id="AddToCart" class="btn button gt_flex gt_align-i-center gt_justify-c-center gt_product-addtocart--button">
                  <span class="gt_product-addtocart--text">
                    {%- if current_variant.available -%}
                      BUY IT NOW!
                    {%- else -%}
                      SOLD OUT
                    {%- endif -%}
                  </span>
                </button>

                <div class="position_hook">
                  
                </div>
                </div>
              
            {% endform %}

