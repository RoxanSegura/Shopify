  <script type="application/ld+json">{%- assign schemaProduct = product -%}
{%- assign schemaVariant = product.selected_or_first_available_variant -%}
{%- assign g_review_count = 0 -%}
{%- assign g_rating_value = 0 -%}
{%- comment -%}
  For Shopify Product Reviews
{%- endcomment -%}
{%- if schemaProduct.metafields.spr.reviews -%}
    {%- assign product_review_array = "reviewCount,ratingValue" | split: ',' -%}
    {%- capture review_data -%}
        {{schemaProduct.metafields.spr.reviews}}
    {%- endcapture -%}
    {%- assign array_builder = "" -%}
    {%- for review_data_item in product_review_array -%}
        {%- capture new_var -%}
          "{{ review_data_item }}"
        {%- endcapture -%}
        {%- assign review_data1 = review_data | split: new_var -%}
        {%- assign review_data2 = review_data1[1] | split: ":" -%}
        {%- assign review_data3 = review_data2[1] | split: '"' -%}
        {%- assign review_data_value = review_data3[1] | plus: 1 | minus: 1 -%}
        {%- assign array_builder = array_builder | append: review_data_value | append: '_' -%}
    {%- endfor -%}

    {%- assign review_data_array = array_builder | split:'_' -%}
    {%- assign g_review_count = review_data_array[0] -%}
    {%- assign g_rating_value = review_data_array[1] -%}
{%- endif -%}
{%- comment -%}
  For Judge Reviews
{%- endcomment -%}
{%- if schemaProduct.metafields.judgeme.badge -%}
    {%- assign product_review_array="data-number-of-reviews,data-average-rating" | split: ',' -%}
    {%- capture review_data -%}
        {{schemaProduct.metafields.judgeme.badge}}
    {%- endcapture -%}
    {%- assign array_builder = "" -%}
    {%- for review_data_item in product_review_array -%}
        {%- assign review_data1 = review_data | split: review_data_item -%}
        {%- assign review_data2 = review_data1[1] | split: "=" -%}
        {%- assign review_data3 = review_data2[1] | split: "'" -%}
        {%- assign review_data_value = review_data3[1] | plus: 1 | minus: 1 -%}
        {%- assign array_builder = array_builder | append:review_data_value | append: '_' -%}
    {%- endfor -%}
    {%- assign review_data_array = array_builder | split: '_' -%}
    {%- assign g_review_count = review_data_array[0] -%}
    {%- assign g_rating_value = review_data_array[1] -%}
{%- endif -%}
{%- comment -%}
  For Loox Reviews
{%- endcomment -%}
{%- if schemaProduct.metafields.loox.num_reviews -%}
    {%- assign g_review_count = schemaProduct.metafields.loox.num_reviews -%}
    {%- assign g_rating_value = schemaProduct.metafields.loox.avg_rating -%}
{%- endif -%}
{%- comment -%}
  Opinew Product Reviews
{%- endcomment -%}
{%- if schemaProduct.metafields.opinew_metafields.reviews_count and schemaProduct.metafields.opinew_metafields.reviews_count != "0" -%}
    {%- assign g_review_count = schemaProduct.metafields.opinew_metafields.reviews_count -%}
    {%- assign g_rating_value = schemaProduct.metafields.opinew_metafields.reviews_average -%}
{%- endif -%}
{%- comment -%}
  For Growave
{%- endcomment -%}
{%- if schemaProduct.metafields.ssw[ssw_rate_data_key] -%}
    {%- assign g_review_count = schemaProduct.metafields.ssw[ssw_rate_data_key].count | plus: 0 -%}
    {%- assign g_rating_value = schemaProduct.metafields.ssw[ssw_rate_data_key].avg | plus: 0 -%}
    {%- assign ssw_product_id = schemaProduct.id -%}
    {%- if schemaProduct.metafields.ssw[ssw_rate_data_key].product_id -%}
        {%- assign ssw_product_id = schemaProduct.metafields.ssw[ssw_rate_data_key].product_id | plus: 0 -%}
    {%- endif -%}
    {%- if ssw_product_id != schemaProduct.id -%}
        {%- assign g_review_count = 0 -%}
    {%- endif -%}
{%- endif -%}
{
  "@context": "http://schema.org/",
  "@type": "Product",
  "name":{{schemaProduct.title | json}},
  "url":{{shop.url | append: schemaProduct.url | json}},
{%- if schemaProduct.featured_image -%}
  {%- assign image_size = schemaProduct.featured_image.width | append: 'x' -%}
  "image": [
    {{schemaProduct.featured_image.src | img_url: image_size | prepend: "https:" | json}}
  ],
{%- endif -%}

  "description": {{schemaProduct.description | default: "" | strip_html | json}},

{%- if schemaProduct.selected_or_first_available_variant.sku != blank -%}
  "sku":{{schemaProduct.selected_or_first_available_variant.sku | json}},
{%- endif -%}

  "brand":{
    "@type": "Thing",
    "name":{{schemaProduct.vendor | json}}
  },
  "mpn": "",

{% if schemaVariant.barcode.size == 12 %}
  "gtin12": {{ schemaVariant.barcode }},
{%- endif -%}

{%- if schemaVariant.barcode.size == 13 -%}
  "gtin13": {{ schemaVariant.barcode }},
{%- endif -%}

{%- if schemaVariant.barcode.size == 14 -%}
  "gtin14": {{ schemaVariant.barcode }},
{%- endif -%}

{%- assign g_review_count = g_review_count | plus: 0 -%}
{%- if g_review_count != 0 -%}
  "aggregateRating": {"@type":"AggregateRating","ratingValue":"{{ g_rating_value }}","reviewCount":"{{ g_review_count }}", "bestRating": "5", "worstRating": "1"},
{%- elsif schemaProduct.metafields.alireviews.seo_rating_review_key_script -%}
  {{schemaProduct.metafields.alireviews.seo_rating_review_key_script}}
{%- endif -%}

  "offers": {
    "@type" : "Offer",

  {%- if schemaVariant.sku != blank -%}
    "sku":{{schemaVariant.sku | json}},
  {%- endif -%}
    "availability" : "http://schema.org/{% if schemaVariant.available %}InStock{% else %}OutOfStock{% endif %}",
    "price" :{{schemaVariant.price | divided_by: 100.00 | json}},
    "priceCurrency" :{{cart.currency.iso_code | json}},
    "priceValidUntil": "{{ 'now' | date: '%s' | plus: 31536000 | date: '%Y-%m-%d' | uri_encode | replace:'+','%20' }}",
    "url" :{{shop.url | append: schemaVariant.url | json}}
  }
}
</script>

</div></section>


<section class="gt_section-4298080 " data-name="Tab Product">
  
