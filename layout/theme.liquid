<!doctype html>
<html class="no-js supports-no-cookies" lang="{{ shop.locale }}">
 {{ 'stylesheet.css.liquid' | asset_url | stylesheet_tag }}
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width,initial-scale=1">
  <link rel="canonical" href="{{ canonical_url }}">

  {% include 'header-settings' %}
  {% include 'header-seo' %}
  {% include 'header-social-meta' %}
  {{ content_for_header }}
  {% include 'header-tag' %}
  {% include 'header-custom' %}

  {{ 'gang-custom-css-shopify--main.css' | asset_url | stylesheet_tag }}
</head>

<body id="{{ page_title | handle }}" class="template-{{ template.name | handle }}">
  {% include 'gemthemes-layout-before' %}
  <main role="main" id="MainContent" >
    {{ content_for_layout }}
  </main>
  {% include 'gemthemes-layout-after' %}
  {% include 'gemthemes-global-widgets' %}
</body>
</html>
